{
  "name": "unplugin-inspector-lib-css",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "pnpm@7.2.1",
  "description": "",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/unplugin-Inspector-lib-css#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/unplugin-Inspector-lib-css.git"
  },
  "bugs": "https://github.com/Simon-He95/unplugin-Inspector-lib-css/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ],
      "vite": [
        "./dist/vite.js",
        "./dist/vite.d.ts"
      ],
      "webpack": [
        "./dist/webpack.js",
        "./dist/webpack.d.ts"
      ],
      "esbuild": [
        "./dist/esbuild.js",
        "./dist/esbuild.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts ./src/vite.ts ./src/webpack.ts ./src/esbuild.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "unocss": "^0.61.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "@antfu/utils": "^0.5.2",
    "@rollup/pluginutils": "^5.1.0",
    "@types/node": "^18.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "pnpm": "^7.2.1",
    "rimraf": "^3.0.2",
    "tsup": "^8.1.0",
    "typescript": "^4.7.4",
    "unocss": "^0.61.0",
    "unplugin": "^1.11.0",
    "vite": "^5.3.2",
    "vitest": "^1.6.0"
  }
}
